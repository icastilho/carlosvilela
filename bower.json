{
  "name": "carlosvilela",
  "version": "0.0.0",
  "authors": [
    "icastilho <italorfc@gmail.com>"
  ],
  "description": "Portifolio Carlos Vilela",
  "main": "js/main.js",
  "moduleType": [
    "globals"
  ],
  "keywords": [
    "CV"
  ],
  "license": "MIT",
  "homepage": "carlosvilela.com.br",
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "src/bower_components",
    "test",
    "tests"
  ],
  "install": {
    "base":  "src/libs",
    "path" : {
      "js": "{name}/js",
      "css": "{name}/css",
      "otf": "{name}/fonts",
      "eot": "{name}/fonts",
      "svg": "{name}/fonts",
      "ttf": "{name}/fonts",
      "woff": "{name}/fonts",
      "woff2": "{name}/fonts"
    },
    "sources" : {
      "imagesloaded" : "src/bower_components/imagesloaded/imagesloaded.pkgd.js",
      "packery" : "src/bower_components/packery/dist/packery.pkgd.js",
      "threesixty-slider" : [
        "src/bower_components/threesixty-slider/src/styles/threesixty.css",
        "src/bower_components/threesixty-slider/src/threesixty.js"
        ],
      "font-awesome" : [
        "src/bower_components/font-awesome/css/font-awesome.css",
        "src/bower_components/font-awesome/fonts/FontAwesome.otf",
        "src/bower_components/font-awesome/fonts/fontawesome-webfont.eot",
        "src/bower_components/font-awesome/fonts/fontawesome-webfont.svg",
        "src/bower_components/font-awesome/fonts/fontawesome-webfont.ttf",
        "src/bower_components/font-awesome/fonts/fontawesome-webfont.woff",
        "src/bower_components/font-awesome/fonts/fontawesome-webfont.woff2"
      ]
    }
  },
  "dependencies": {
    "packery": "~1.4.1",
    "angular": "~1.3.15",
    "angular-route": "~1.3.15",
    "angular-ui-router": "~0.2.14",
    "font-awesome": "~4.3.0",
    "imagesloaded": "~3.1.8",
    "jquery": "~2.1.4",
    "jquery-ui": "~1.11.4",
    "bootstrap": "~3.3.4",
    "angular-animate": "~1.3.15",
    "threesixty-slider": "~2.0.5"
  }
}
